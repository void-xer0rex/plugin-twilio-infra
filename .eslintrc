{
  "extends": [
    "twilio"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "no-console": "off",
    "import/no-unresolved": ["warn", {"amd": true}],
    "prefer-named-capture-group": "off"
  }
}